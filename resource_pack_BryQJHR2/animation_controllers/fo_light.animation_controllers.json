{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.fo": {
            "states": {
                "default": {
					"animations":[
                        "nolight"
                    ],
                    "transitions":[
                        {"lighting": "!query.is_baby"}
                    ]
                },
                "lighting": {
                    "animations":[
                        "light"
                    ],
                    "transitions":[
                        {"lighting2": "query.all_animations_finished"}
                    ]
                },
				"lighting2": {
                    "animations":[
                        "zhuan"
                    ]
                }
            }
        }
    }
}